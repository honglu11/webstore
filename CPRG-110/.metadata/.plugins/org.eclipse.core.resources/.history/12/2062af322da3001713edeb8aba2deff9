package ca.sait.bookstore.model;

public class Beverage extends Item {
	private char size;
	
	public Beverage(String name, double price) {
		this(name, price, 'M');
	}
	
	public Beverage(String name, double price, String size) {
		super(name, price);
		this.size = size;
		
	}
	
	
